###  http://rdf.ebi.ac.uk/terms/biosd/has-sample-attribute

biosdterms:has-sample-attribute rdf:type :ObjectProperty ;
                                
                                rdfs:label "sample attribute"@en ;
                                
                                obo:IAO_0000115 "Relates a sample or sample group to one of its attributes, not necessarily an attributes that is strictly related to biological qualities. For instance: provider name, identification code. This corresponds to attribute columns from formats like SampleTab or MAGE-TAB, such that the column header doesn't necessarily  refer to a biological characteristic explicitly,e.g., 'Comment[Sample Provider]'.  This relation might establish a not-too-formal associatios. For instance, an association from sample group to 'sample provider' might mean the group contains some samples coming from that provider."@en ;
                                
                                rdfs:subPropertyOf obo:RO_0000086 ,
                                                   sio:SIO_000008 ;
                                
                                rdfs:domain [ rdf:type :Class ;
                                              :unionOf ( biosdterms:Sample
                                                         biosdterms:SampleGroup
                                                       )
                                            ] .



###  http://rdf.ebi.ac.uk/terms/biosd/parent-of

biosdterms:parent-of rdf:type :ObjectProperty ;
                     
                     rdfs:label "parent of" ;
                     
                     obo:IAO_0000115 "x parent-of y iff the organism x is the biological parent of y. " ;
                     
                     rdfs:range obo:OBI_0100026 ;
                     
                     rdfs:domain obo:OBI_0100026 ;
                     
                     rdfs:subPropertyOf obo:RO_0002226 ;
                     
                     :inverseOf biosdterms:child-of ;
                     
                     rdfs:range sio:SIO_010000 ;
                     
                     rdfs:domain sio:SIO_010000 .



###  http://rdf.ebi.ac.uk/terms/biosd/sample-child

biosdterms:sample-child rdf:type :ObjectProperty ;
                        
                        rdfs:label "sample-child of" ;
                        
                        obo:IAO_0000115 "A sample x is sample-child-of y iff x derives from organism xo, y derives from organism yo and xo is the biological child of yo. In other words, the two samples are taken from two orgranisms having a biological parentage relationship. We have used OWL-2 chain axioms, PROV-O, RO to formalise this relationship." ;
                        
                        rdfs:domain biosdterms:Sample ;
                        
                        rdfs:range biosdterms:Sample ;
                        
                        rdfs:subPropertyOf sio:SIO_000244 ,
                                           prov:wasDerivedFrom ;
                        
                        :propertyChainAxiom ( biosdterms:derived-from-organism
                                              biosdterms:child-of
                                              [ :inverseOf biosdterms:derived-from-organism
                                              ]
                                            ) .



###  http://rdf.ebi.ac.uk/terms/biosd/sample-parent-of

biosdterms:sample-parent-of rdf:type :ObjectProperty ;
                            
                            rdfs:label "sample-parent of" ;
                            
                            obo:IAO_0000115 "A sample x is sample-parent-of y iff x derives from organism xo, y derives from organism yo and xo is the biological parent of yo. In other words, the two samples are taken from two organisms having a biological parentage relationship. We have used OWL-2 chain axioms, PROV-O, RO to formalise this relationship." ;
                            
                            rdfs:domain biosdterms:Sample ;
                            
                            rdfs:range biosdterms:Sample ;
                            
                            :inverseOf biosdterms:sample-child ;
                            
                            rdfs:subPropertyOf sio:SIO_000245 ,
                                               prov:hadDerivation ;
                            
                            :propertyChainAxiom ( biosdterms:derived-from-organism
                                                  biosdterms:parent-of
                                                  [ :inverseOf biosdterms:derived-from-organism
                                                  ]
                                                ) .



